# KayTee E2E Tests Environment Variables
# Copy this file to .env.local and fill in values

# ============================================
# Application URLs
# ============================================

# KayTee Tropical Dashboard (Business)
KAYTEE_TROPICAL_URL=https://kayteetropical.com
KAYTEE_TROPICAL_DEV_URL=https://dev.kayteetropical.com

# Quality Metrics Dashboard (Open-source)
QUALITY_METRICS_URL=https://qa.khannara.dev

# Digital Portfolio
PORTFOLIO_URL=https://khannara.dev

# KayTee Tropical Hub (Operations)
KAYTEE_HUB_URL=https://hub.kayteetropical.com

# ============================================
# Metrics API (for reporting results)
# ============================================
METRICS_API_URL=https://api.khannara.dev
METRICS_API_KEY=your_api_key_here

# ============================================
# Test User Credentials (KayTee Tropical)
# ============================================
# These are for testing authenticated flows
# Use the credentials from kaytee-tropical-dashboard/.env.local
LOCALHOST_EMAIL=khannara.phay@gmail.com
LOCALHOST_PASSWORD=password

# Preview environment credentials (for dev.kayteetropical.com)
PREVIEW_ADMIN_EMAIL=khannara.phay@gmail.com
PREVIEW_ADMIN_PASSWORD=your_preview_password

# Production credentials (for kayteetropical.com)
PROD_ADMIN_EMAIL=khannara.phay@gmail.com
PROD_ADMIN_PASSWORD=your_prod_password

# ============================================
# Database (for test data setup/teardown)
# ============================================
# MongoDB (KayTee Tropical)
MONGODB_URI=mongodb+srv://...
MONGODB_DATABASE=kaytee-tropical-dashboard-test

# Supabase (Quality Metrics, Hub)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key

# ============================================
# CI/CD Environment
# ============================================
CI=false
